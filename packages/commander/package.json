{
    "name": "@decopro/commander",
    "version": "1.1.0",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "src/index.ts",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsup",
        "clean": "rm -rf ./.turbo && rm -rf ./dist && rm -rf ./node_modules",
        "build:watch": "tsup --watch",
        "dev": "tsup --watch --sourcemap",
        "test": "jest --passWithNoTests",
        "build:prod": "NODE_ENV=production tsup",
        "lint": "echo \"No linting configured for this package\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@decopro/core": "workspace:*",
        "zod": "^3.25.67"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "@types/jest": "^29.5.12",
        "ts-jest": "^29.1.2"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src"
        ],
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts",
            "!src/**/*.d.ts"
        ]
    }
}
